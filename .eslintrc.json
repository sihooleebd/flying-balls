{
    "env": {
        "browser": true,
        "es2022": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "plugins":["prettier"],
    "rules": {
        //"indent": [2, 2]
        "no-unused-vars"  : "warn"
        // "prettier/prettier": "error"
    },
    "ignorePatterns": [
        "node_modules/"
    ]
}